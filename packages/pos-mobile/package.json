{
  "name": "@park-angel/pos-mobile",
  "version": "1.0.0",
  "description": "Park Angel POS Mobile App - React Native with Expo for iOS and Android POS operations",
  "main": "expo-router/entry",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "build": "tsc",
    "build:android": "eas build --platform android",
    "build:ios": "eas build --platform ios",
    "lint": "eslint src --ext .js,.jsx,.ts,.tsx --report-unused-disable-directives --max-warnings 0 --no-error-on-unmatched-pattern",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "clean": "rm -rf node_modules .expo"
  },
  "dependencies": {
    "@park-angel/shared": "file:../shared",
    "@react-native-async-storage/async-storage": "^1.21.0",
    "@react-native-community/netinfo": "^11.2.0",
    "@supabase/supabase-js": "^2.39.0",
    "@tanstack/react-query": "^5.24.0",
    "date-fns": "^3.3.0",
    "expo": "^53.0.20",
    "expo-barcode-scanner": "~12.9.0",
    "expo-camera": "~14.0.0",
    "expo-constants": "~15.4.0",
    "expo-device": "~5.9.0",
    "expo-file-system": "~16.0.0",
    "expo-image-picker": "~14.7.0",
    "expo-local-authentication": "~13.8.0",
    "expo-location": "~16.5.0",
    "expo-network": "~5.8.0",
    "expo-notifications": "^0.31.4",
    "expo-print": "~12.8.0",
    "expo-router": "^3.4.0",
    "expo-secure-store": "~12.8.0",
    "expo-sharing": "~11.10.0",
    "expo-splash-screen": "^0.30.10",
    "expo-sqlite": "~13.0.0",
    "expo-status-bar": "~1.11.0",
    "lodash": "^4.17.21",
    "react": "18.2.0",
    "react-native": "^0.73.11",
    "react-native-gesture-handler": "~2.14.0",
    "react-native-keychain": "^8.2.0",
    "react-native-mlkit-ocr": "^0.3.0",
    "react-native-modal": "^13.0.1",
    "react-native-netinfo": "@react-native-community/netinfo",
    "react-native-paper": "^5.12.0",
    "react-native-permissions": "^4.1.0",
    "react-native-reanimated": "~3.6.0",
    "react-native-safe-area-context": "4.8.2",
    "react-native-screens": "~3.29.0",
