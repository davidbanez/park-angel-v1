{
  "name": "@park-angel/pos-mobile",
  "version": "1.0.0",
  "description": "Park Angel POS Mobile App - React Native with Expo for iOS and Android POS operations",
  "main": "expo-router/entry",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "build": "expo build",
    "lint": "eslint src --ext .js,.jsx,.ts,.tsx --report-unused-disable-directives --max-warnings 0 --no-error-on-unmatched-pattern",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf node_modules .expo"
  },
  "dependencies": {
    "expo": "~49.0.0",
    "expo-router": "^2.0.0",
    "react": "18.2.0",
    "react-native": "0.72.3",
    "react-native-safe-area-context": "4.6.3",
    "react-native-screens": "~3.22.0",
    "zustand": "^4.4.1",
    "@tanstack/react-query": "^4.32.6",
    "@supabase/supabase-js": "^2.33.1",
    "react-native-url-polyfill": "^2.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@types/react": "~18.2.14",
    "@types/react-native": "~0.72.2",
    "typescript": "^5.1.3"
  },
  "private": true
}
