{
  "name": "@park-angel/operator-dashboard",
  "version": "1.0.0",
  "description": "Park Angel Operator Dashboard - Web and mobile application for parking operators",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:mobile": "expo start",
    "build": "tsc --project tsconfig.build.json && vite build",
    "build:mobile": "expo build",
    "preview": "vite preview",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0 --no-error-on-unmatched-pattern",
    "type-check": "tsc --noEmit",
    "test": "vitest --run",
    "test:watch": "vitest",
    "clean": "rm -rf dist node_modules .expo"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.14.2",
    "zustand": "^4.4.1",
